#include <stdio.h>
#include <string.h>
#include <float.h>
int main(void)
{
    double a=1.0/3.0;
    float b=1.0/3.0;
    printf("%.6lf  %.12lf %.16lf \n",a,a,a);
    printf("%.6f  %.12f  %.16f\n",b,b,b);
    printf("FLT_DIG:%lf\n", FLT_DIG);
    printf("DBL_DIG:%lf\n", DBL_DIG);
    return 0;

    
           
   
}
